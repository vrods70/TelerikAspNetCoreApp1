    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-jasmine": "^1.0.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.1.2",
    "rimraf": "^2.5.4",

    "@types/node": "^7.0.22",
    "@types/jasmine": "2.5.47"
  },
